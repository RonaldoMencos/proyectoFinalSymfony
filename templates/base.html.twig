<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        {% block title %}<title>Welcome!</title>{% endblock %}</>
        {% block stylesheets %}{% endblock %}
        <link rel="stylesheet" href="{{asset('../vendor/twbs/bootstrap/dist/css/bootstrap.min.css')}}"
    </head>
    <body>
        <header>
            <nav class="navbar navbar-expand-lg bg-dark border-bottom border-body" data-bs-theme="dark">
                <div class="container-fluid">
                  <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarTogglerDemo01" aria-controls="navbarTogglerDemo01" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                  </button>
                  <div class="collapse navbar-collapse" id="navbarTogglerDemo01">
                    <a class="navbar-brand" href="{{ path('inicio') }}">Proyecto Final</a>
                    <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                      <li class="nav-item">
                        <a class="nav-link" aria-current="page" href="{{ path('inicio') }}">Inicio</a>
                      </li>
                      <li class="nav-item">
                        <a class="nav-link" href="{{ path('getAlumnos') }}">Alumnos</a>
                      </li>
                      <li class="nav-item">
                        <a class="nav-link" href="{{ path('getNotas') }}">Notas</a>
                      </li>
                      <li class="nav-item">
                        <a class="nav-link" href="{{ path('getSemestres') }}">Semestres</a>
                      </li>
                      <li class="nav-item">
                        <a class="nav-link" href="{{ path('getCarreras') }}">Carreras</a>
                      </li>
                      <li class="nav-item">
                        <a class="nav-link" href="{{ path('getCursos') }}">Cursos</a>
                      </li>
                      <li class="nav-item">
                        <a class="nav-link" href="{{ path('getSecciones') }}">Secciones</a>
                      </li>
                      <li class="nav-item">
                        <a class="nav-link" href="{{ path('getAllAlumnos') }}">Reporte Alumnos</a>
                      </li>
                      <li class="nav-item">
                        <a class="nav-link" href="{{ path('getAllNotas') }}">Reporte Notas</a>
                      </li>
                      <li class="nav-item">
                        <a class="nav-link" href="{{ path('getAlumnoByFilters') }}">Informaci√≥n Alumno</a>
                      </li>
                    </ul>
                  </div>
                </div>
              </nav>
        </header>
        <div class="container mt-2">
          {% for notice in app.flashes('error') %}
          <div class="alert alert-danger alert-dismissible fade show" role="alert">
            <strong>Error!</strong> {{notice}}
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
          </div>
          {% endfor %}
          {% for notice in app.flashes('success') %}
          <div class="alert alert-success alert-dismissible fade show" role="alert">
            <strong>Correcto!</strong> {{notice}}
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
          </div>
          {% endfor %}
          {% block body %}{% endblock %}
        </div>
        {% block javascripts %}
            <script src="{{asset('../vendor/twbs/bootstrap/dist/js/bootstrap.bundle.min.js')}}" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>

        {% endblock %}
    </body>
</html>
